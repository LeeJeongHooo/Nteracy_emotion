{
  "compilerOptions": {
    "jsx": "react",
    "module": "ES6",
    "target": "ES6",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "baseUrl": ".",
    "paths": {
      "@src/*": ["src/*"]
    }
  },
  "include": ["src/**/*.ts", "src/**/*.tsx", "emotion.d.ts"],
  "exclude": ["node_modules"]
}
